# AI Agent Configuration
ENVIRONMENT=production

# Asterisk ARI Configuration (Recommended)
ASTERISK_ARI_HOST=localhost
ASTERISK_ARI_PORT=8088
ASTERISK_ARI_USERNAME=asterisk
ASTERISK_ARI_PASSWORD=your_ari_password
ASTERISK_LANGUAGE=de

# Asterisk AMI Configuration (Legacy - for AGI compatibility)
ASTERISK_HOST=localhost
ASTERISK_PORT=5038
ASTERISK_USERNAME=admin
ASTERISK_SECRET=your_asterisk_secret
ASTERISK_CONTEXT=outbound-calls
ASTERISK_CALLER_ID=+4912345678

# Deepgram Configuration
DEEPGRAM_API_KEY=your_deepgram_api_key
DEEPGRAM_MODEL=nova-2
DEEPGRAM_LANGUAGE=de

# OpenAI Configuration (for Whisper and GPT)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview

# Anthropic Configuration (alternative AI provider)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/aiagc_calls

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Call Configuration
MAX_CALL_DURATION=600
SILENCE_TIMEOUT=5
SPEECH_TIMEOUT=10
MAX_RETRIES=3

# Lead Scoring Configuration
LEAD_SCORE_THRESHOLD=70
QUALIFICATION_CRITERIA=investment_interest,budget_available,decision_maker

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/aiagc.log

# Audio Configuration
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1
AUDIO_FORMAT=wav

# Security
SECRET_KEY=your_secret_key_here_change_in_production
API_TOKEN=your_api_token_here
